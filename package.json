    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "^2.0.1",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
